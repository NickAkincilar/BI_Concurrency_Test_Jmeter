        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="BI Query Daily" enabled="true">
          <stringProp name="dataSource">biusers</stringProp>
          <stringProp name="query">SELECT 
    --TEST#1
    O_ORDERPRIORITY,
    P_NAME as PartName, 
    sum((L_EXTENDEDPRICE * (1-L_DISCOUNT)) * (1+L_TAX)) as SalesTotal, 
    avg(L_DISCOUNT) as AvgDisc, count(L_LINENUMBER) as Orders,
    RANDOM() as SomeNo
from SNOWFLAKE_SAMPLE_DATA.TPCH_SF10.ORDERS O
    LEFT JOIN SNOWFLAKE_SAMPLE_DATA.TPCH_SF10.LINEITEM L ON O_ORDERKEY = L_ORDERKEY
    LEFT JOIN SNOWFLAKE_SAMPLE_DATA.TPCH_SF10.PART P ON L_PARTKEY = P_PARTKEY
WHERE
        ${MYFILTER} -- This line needs to be a dynamic list of valid filters  
group by 1,2
order by 3 desc;
</stringProp>
          <stringProp name="queryArguments"></stringProp>
          <stringProp name="queryArgumentsTypes"></stringProp>
          <stringProp name="queryTimeout"></stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="resultSetHandler">Store as String</stringProp>
